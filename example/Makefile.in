CC	= @CC@
CFLAGS	= @CFLAGS@
LD	= @CC@
LDFLAGS	= 
LIBS	= 
OBJS	= module1.o module2.o 

all: example

.c.o:
	$(CC) $(CFLAGS) -c -o $@ $<

example: $(OBJS)
	$(CC) -o $@ $(LDFLAGS) $(OBJS) $(LIBS)

clean:
	-rm -f example $(OBJS)

distclean : clean

install:
uninstall:
